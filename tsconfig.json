{
  "extends": "./tsconfig_ex",
  "exclude": [
    "build/**/*",
    "**/build/**/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@polkadot/apps-config/*": [ "packages/apps-config/src/*" ],
      "@polkadot/apps-routing": [ "packages/apps-routing/src" ],
      "@polkadot/apps-routing/*": [ "packages/apps-routing/src/*" ],
      "@polkadot/app-accounts": [ "packages/page-accounts/src" ],
      "@polkadot/app-accounts/*": [ "packages/page-accounts/src/*" ],
      "@polkadot/app-claims": [ "packages/page-claims/src" ],
      "@polkadot/app-claims/*": [ "packages/page-claims/src/*" ],
      "@polkadot/app-contracts": [ "packages/page-contracts/src" ],
      "@polkadot/app-contracts/*": [ "packages/page-contracts/src/*" ],
      "@polkadot/app-extrinsics": [ "packages/page-extrinsics/src" ],
      "@polkadot/app-extrinsics/*": [ "packages/page-extrinsics/src/*" ],
      "@polkadot/app-explorer": [ "packages/page-explorer/src" ],
      "@polkadot/app-explorer/*": [ "packages/page-explorer/src/*" ],
      "@polkadot/app-settings": [ "packages/page-settings/src" ],
      "@polkadot/app-settings/*": [ "packages/page-settings/src/*" ],
      "@polkadot/app-staking": [ "packages/page-staking/src" ],
      "@polkadot/app-staking/*": [ "packages/page-staking/src/*" ],
      "@polkadot/app-storage": [ "packages/page-storage/src" ],
      "@polkadot/app-storage/*": [ "packages/page-storage/src/*" ],
      "@polkadot/app-sudo": [ "packages/page-sudo/src" ],
      "@polkadot/app-sudo/*": [ "packages/page-sudo/src/*" ],
      "@polkadot/app-toolbox": [ "packages/page-toolbox/src" ],
      "@polkadot/app-toolbox/*": [ "packages/page-toolbox/src/*" ],
      "@polkadot/react-api": [ "packages/react-api/src" ],
      "@polkadot/react-api/*": [ "packages/react-api/src/*" ],
      "@polkadot/react-components": [ "packages/react-components/src" ],
      "@polkadot/react-components/*": [ "packages/react-components/src/*" ],
      "@polkadot/react-hooks": [ "packages/react-hooks/src" ],
      "@polkadot/react-hooks/*": [ "packages/react-hooks/src/*" ],
      "@polkadot/react-params": [ "packages/react-params/src" ],
      "@polkadot/react-params/*": [ "packages/react-params/src/*" ],
      "@polkadot/react-query": [ "packages/react-query/src" ],
      "@polkadot/react-query/*": [ "packages/react-query/src/*" ],
      "@polkadot/react-signer": [ "packages/react-signer/src" ],
      "@polkadot/react-signer/*": [ "packages/react-signer/src/*" ],
      "@polkadot/api": ["packages/api/src"],
      "@polkadot/api/*": ["packages/api/src/*"],
      "@polkadot/api-contract": ["packages/api-contract/src"],
      "@polkadot/api-contract/*": ["packages/api-contract/src/*"],
      "@polkadot/api-derive": ["packages/api-derive/src"],
      "@polkadot/api-derive/*": ["packages/api-derive/src/*"],
      "@polkadot/jsonrpc": ["packages/jsonrpc/src"],
      "@polkadot/jsonrpc/*": ["packages/jsonrpc/src/*"],
      "@polkadot/metadata": ["packages/metadata/src"],
      "@polkadot/metadata/*": ["packages/metadata/src/*"],
      "@polkadot/rpc-core": ["packages/rpc-core/src"],
      "@polkadot/rpc-core/*": ["packages/rpc-core/src/*"],
      "@polkadot/rpc-provider": ["packages/rpc-provider/src"],
      "@polkadot/rpc-provider/*": ["packages/rpc-provider/src/*"],
      "@polkadot/types": ["packages/types/src"],
      "@polkadot/types/*": ["packages/types/src/*"],
      "@polkadot/react-darwinia": ["packages/react-darwinia/src"],
      "@polkadot/react-darwinia/*": ["packages/react-darwinia/src/*"],
      "@polkadot/util": ["packages/util/src"],
      "@polkadot/util/*": ["packages/util/src/*"],
      "@polkadot/util-crypto": ["packages/util-crypto/src"],
      "@polkadot/util-crypto/*": ["packages/util-crypto/src/*"],
      "@polkadot/keyring": ["packages/keyring/src"],
      "@polkadot/keyring/*": ["packages/keyring/src/*"],
      "@polkadot/dev": ["packages/dev/src"],
      "@polkadot/dev/*": ["packages/dev/src/*"],
      "@polkadot/ui-settings": [ "packages/ui-settings/src" ],
      "@polkadot/ui-settings/*": [ "packages/ui-settings/src/*" ],
      "@polkadot/api/augment": ["packages/react-darwinia/src/interfaces/augment/augment-api.ts"],
      "@polkadot/types/augment": ["packages/react-darwinia/src/interfaces/augment/augment-types.ts"]
    },
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@polkadot/ts",
      "./node_modules/@types"
    ]
  }
}
